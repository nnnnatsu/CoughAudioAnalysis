{
  "version": 2,
  "builds": [
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["MODEL_CNN.h5"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "streamlit_app.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "commands": [
      "sh setup.sh"
    ]
  }
}
