{
  "version": 2,
  "builds": [
    {
      "src": "install_packages.py",
      "use": "@vercel/python"
    },
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["MODEL_CNN.h5"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/streamlit_app.py"
    }
  ]
}
